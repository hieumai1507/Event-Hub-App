{
  "expo": {
    "name": "EventHub",
    "slug": "EventHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  },
  "assets": [
    "./assets/fonts/AirbnbCereal_W_Bd.otf",
    "./assets/fonts/AirbnbCereal_W_Bk.otf",
    "./assets/fonts/AirbnbCereal_W_Blk.otf",
    "./assets/fonts/AirbnbCereal_W_Lt.otf",
    "./assets/fonts/AirbnbCereal_W_Md.otf",
    "./assets/fonts/AirbnbCereal_W_XBd.otf"
  ]
}
